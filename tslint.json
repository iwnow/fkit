{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [140]
        },
        "quotemark": [true, "single", "jsx-double"],
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    },
    "jsRules": {
        "max-line-length": {
            "options": [140]
        }
    }
}